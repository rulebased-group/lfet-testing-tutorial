LFET:

    Project:

        Url:
        # List of global unique project ID's
        - https://github.com/rulebased-group/lfet-testing-tutorial.git

        FileNamePattern:
        # List of regular expressions for allowed file names
        # incl. common 'save as' and export formats
        - ^(?i)BenutzerkontoErstellungOnlineshop[.].*$
        - ^(?i)Spendenaufruf[.].*$
        - ^(?i)ZahlungswegeOnlineshop[.].*$
        - ^(?i)RechnungsbetragOnlineshop[.].*$

        DirectoryNamePattern:
        # List of regular expressions for allowed directory names
        # relative to the project content root
        # use always '/' as file separator
        - ^(?i)[/]?testing-tutorial-[56]/01-initial[/]?$
        - ^(?i)[/]?testing-tutorial-[567]/02-updated[/]?$
        - ^(?i)[/]?testing-tutorial-[89][/]?$