# This file must not be changed.
#
# The content format is YAML and its checksum value must match
# with the checksum value stored in the corresponding license file.
#
# Any CER's are welcome here: dirk.o.schweier@rulebased.group

LFET:
  
  Project:
    
    # the global unique project id
    Url: https://github.com/rulebased-group/lfet-testing-tutorial.git
    
    DecisionTables:
      
      # list of regular expressions for allowed decision table file names 
      # incl. common 'save as' formats
      NamePattern:
        - ^BenutzerkontoErstellungOnlineshop[.]lfet$

      # list of regex relative to this '.lfet' file's folder
      # use always '/' as file separator
      PathPattern: 
        - ^[/]?testing-tutorial-5/01-initial[/]?$
        - ^[/]?testing-tutorial-5/02-updated[/]?$
